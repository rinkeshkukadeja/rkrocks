<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="collections.Employee_collection" id="abc">
		<property name="l1">
			<list>
				<ref bean="adr1"></ref>
				<ref bean="adr2"></ref>
			</list>
		</property>
	</bean>

	<bean class="com.scp.pratice.hibernate.spring.Address" id="adr1">
		<property name="city" value="kop"></property>
		<property name="Street" value="tarabai"></property>
	</bean>
	<bean class="com.scp.pratice.hibernate.spring.Address" id="adr2">
		<property name="city" value="pune"></property>
		<property name="Street" value="tarabai"></property>
	</bean>

</beans>
   